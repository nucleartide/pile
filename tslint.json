{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "semicolon": [true, "never"],
    "class-name": false,
    "interface-name": [true, "never-prefix"],
    "curly": [true, "ignore-same-line"],
    "typedef": [
      true,
      "call-signature",
      "arrow-call-signature"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-template",
      "avoid-escape"
    ]
  },
  "rulesDirectory": []
}
